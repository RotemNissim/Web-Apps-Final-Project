<h1>Create dish</h1>
<div class="form-horizontal">
    <div class="form-group">
        <div class="col-md-3">
            <label class="control-label">Name</label>
            <input type="text" class="form-control" id="name" name="name">
            <span class="help-block"></span>
            <div id="name-error" class="help-block"></div>
            <label class="control-label">Price</label>
            <input type="text" class="form-control" id="price" name="price">
            <span class="help-block"></span>
            <div id="price-error" class="help-block"></div>
            <label class="control-label">Description</label>
            <input type="text" class="form-control" id="description" name="description">
            <span class="help-block"></span>
            <div id="description-error" class="help-block"></div>
            <label class="control-label">Allergenics</label>
            <input type="text" class="form-control" id="allergenics" name="allergenics">
            <span class="help-block"></span>
            <div id="allergenics-error" class="help-block"></div>
            <label class="control-label">Chef</label>
            <input type="text" class="form-control" id="chef" name="chef">
            <span class="help-block"></span>
            <div id="chef-error" class="help-block"></div>
            <label class="control-label">Tags</label>
            <input type="text" class="form-control" id="tags" name="tags">
            <span class="help-block"></span>
            <div id="tags-error" class="help-block"></div>
            <label class="control-label">Type</label>
            <input type="text" class="form-control" id="type" name="type">
            <span class="help-block"></span>
        </div>
        <div class="col-md-3">
            <label class="control-label">Image</label>
            <input type="file" class="form-control" id="image" name="image">
            <span class="help-block"></span>
            <div id="image-error" class="help-block"></div>
            <label class="control-label">Category</label>
            <input type="text" class="form-control" id="category" name="category">
            <span class="help-block"></span>
            <div id="category-error" class="help-block"></div>
            <label class="control-label">Rating</label>
            <input type="text" class="form-control" id="rating" name="rating">
            <span class="help-block"></span>
            <div id="rating-error" class="help-block"></div>
            <label class="control-label">Rating Count</label>
            <input type="text" class="form-control" id="ratingCount" name="ratingCount">
            <span class="help-block"></span>
            <div id="ratingCount-error" class="help-block"></div>
        </div>

    </div>
    <div class="form-group">
        <div class="col-md-offset-3 col-md-9">
            <button onclick="createDish()" class="btn btn-default">Create</button>
        </div>
    </div>

</div>
